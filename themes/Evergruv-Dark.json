      "name": "Arguments & Variables",
      "scope": "variable.argument, variable, support.variable, meta.definition.variable",
      "settings": {
        "foreground": "#DDC7A1"
      }
    },
    {
      "name": "Builtin Primitive Types",
      "scope": "storage.type.built-in",
      "settings": {
        "foreground": "#D3869B"
      }
    },
    {
      "name": "Constants",
      "scope": "constant, variable.language",
      "settings": {
        "foreground": "#D8A657"
      }
    },
    {
      "name": "Comments",
      "scope": "comment, string.comment, punctuation.definition.comment",
      "settings": {
        "foreground": "#7C6F64"
      }
    },
    {
      "name": "Function Definitions",
      "scope": "storage.type.function",
      "settings": {
        "foreground": "#EA6962"
      }
    },
    {
      "name": "Functions",
      "scope": "entity.name.function, support.function, meta.function, meta.function-call.generic, meta.definition.method",
      "settings": {
        "foreground": "#A9B665"
      }
    },
